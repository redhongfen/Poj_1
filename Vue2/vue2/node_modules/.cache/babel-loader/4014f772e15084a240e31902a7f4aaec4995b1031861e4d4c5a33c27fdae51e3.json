{"ast":null,"code":"export default {\n  name: \"ShoppingView\",\n  data() {\n    return {\n      tableData: [],\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 5,\n      // 每页显示的个数\n      goodsName: '',\n      total: 0,\n      fromVisible: false,\n      form: {\n        goodsName: '',\n        price: '',\n        avatar: '' // 初始化 avatar\n      },\n      user: JSON.parse(localStorage.getItem('honey-user') || '{}'),\n      rules: {\n        goodsName: [{\n          required: true,\n          message: '请输入商品名',\n          trigger: 'blur'\n        }],\n        price: [{\n          required: true,\n          message: '请输入价格',\n          trigger: 'blur'\n        }]\n      },\n      ids: []\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    delBatch() {\n      if (!this.ids.length) {\n        this.$message.warning('请选择数据');\n        return;\n      }\n      this.$confirm('您确认批量删除这些数据吗？', '确认删除', {\n        type: \"warning\"\n      }).then(() => {\n        this.$request.delete('/goods/delete/batch', {\n          data: this.ids\n        }).then(res => {\n          if (res.code === '200') {\n            this.$message.success('操作成功');\n            this.load(1);\n          } else {\n            this.$message.error(res.msg);\n          }\n        });\n      }).catch(() => {});\n    },\n    handleSelectionChange(rows) {\n      this.ids = rows.map(v => v.id);\n    },\n    del(id) {\n      this.$confirm('您确认删除吗？', '确认删除', {\n        type: \"warning\"\n      }).then(() => {\n        this.$request.delete('/goods/delete/' + id).then(res => {\n          if (res.code === '200') {\n            this.$message.success('操作成功');\n            this.load(1);\n          } else {\n            this.$message.error(res.msg);\n          }\n        });\n      }).catch(() => {});\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row)); // 深拷贝\n      this.fromVisible = true; // 打开弹窗\n    },\n    handleAdd() {\n      this.form = {\n        goodsName: '',\n        price: '',\n        avatar: ''\n      }; // 新增时清空数据\n      this.fromVisible = true; // 打开弹窗\n    },\n    save() {\n      this.$refs.formRef.validate(valid => {\n        if (valid) {\n          this.$request({\n            url: this.form.id ? '/file/update' : '/goods/add',\n            method: this.form.id ? 'PUT' : 'POST',\n            data: this.form\n          }).then(res => {\n            if (res.code === '200') {\n              this.$message.success('保存成功');\n              this.load(1);\n              this.fromVisible = false;\n            } else {\n              this.$message.error(res.msg);\n            }\n          });\n        }\n      });\n    },\n    reset() {\n      this.goodsName = '';\n      this.load();\n    },\n    load(pageNum) {\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/goods/selectByPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          goodsName: this.goodsName\n        }\n      }).then(res => {\n        this.tableData = res.data.list;\n        this.total = res.data.total; // 确保这里是 total\n      });\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    },\n    handleAvatarSuccess(response) {\n      // 将上传的图片链接赋给 form.avatar\n      this.form.avatar = response.data; // 确保 response.data 是图片的 URL\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","pageNum","pageSize","goodsName","total","fromVisible","form","price","avatar","user","JSON","parse","localStorage","getItem","rules","required","message","trigger","ids","created","load","methods","delBatch","length","$message","warning","$confirm","type","then","$request","delete","res","code","success","error","msg","catch","handleSelectionChange","rows","map","v","id","del","handleEdit","row","stringify","handleAdd","save","$refs","formRef","validate","valid","url","method","reset","get","params","list","handleCurrentChange","handleAvatarSuccess","response"],"sources":["src/views/manager/ShoppingView.vue"],"sourcesContent":["<template>\r\n    <div class=\"\">\r\n        <div>\r\n            <el-input style=\"width: 200px\" placeholder=\"查询商品名\" v-model=\"goodsName\"></el-input>\r\n            <el-button type=\"primary\" @click=\"load(1)\">查询</el-button>\r\n            <el-button type=\"info\" @click=\"reset\">重置</el-button>\r\n        </div>\r\n        <div style=\"margin: 10px 0\">\r\n            <el-button type=\"primary\" plain @click=\"handleAdd\">新增</el-button>\r\n            <el-button type=\"danger\" plain @click=\"delBatch\">批量删除</el-button>\r\n        </div>\r\n        <el-table :data=\"tableData\" stripe :header-cell-style=\"{ backgroundColor: 'aliceblue', color: '#666' }\" @selection-change=\"handleSelectionChange\">\r\n            <el-table-column type=\"selection\" width=\"55\" align=\"center\"></el-table-column>\r\n            <el-table-column label=\"ID\" prop=\"id\" align=\"center\"></el-table-column>\r\n            <el-table-column label=\"商品名\" prop=\"goodsName\" align=\"center\"></el-table-column>\r\n            <el-table-column label=\"价格\" prop=\"price\" align=\"center\"></el-table-column>\r\n            <el-table-column label=\"头像\">\r\n                <template v-slot=\"scope\">\r\n                    <div style=\"display: flex; align-items: center\">\r\n                        <el-image style=\"width: 50px; height: 50px;\" v-if=\"scope.row.avatar\" :src=\"scope.row.avatar\" :preview-src-list=\"[scope.row.avatar]\"></el-image>\r\n                    </div>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"操作\" align=\"center\">\r\n                <template v-slot=\"scope\">\r\n                    <el-button size=\"mini\" type=\"primary\" plain @click=\"handleEdit(scope.row)\">编辑</el-button>\r\n                    <el-button size=\"mini\" type=\"danger\" plain @click=\"del(scope.row.id)\">删除</el-button>\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n    \r\n        <el-dialog title=\"信息\" :visible.sync=\"fromVisible\" width=\"30%\">\r\n            <el-form :model=\"form\" label-width=\"80px\" style=\"padding-right: 20px\" :rules=\"rules\" ref=\"formRef\">\r\n                <el-form-item label=\"商品名\" prop=\"goodsName\">\r\n                    <el-input v-model=\"form.goodsName\" placeholder=\"商品名\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"价格\" prop=\"price\">\r\n                    <el-input v-model=\"form.price\" placeholder=\"价格\"></el-input>\r\n                </el-form-item>\r\n    \r\n                <el-form-item label=\"头像\">\r\n                    <el-upload\r\n                        class=\"avatar-uploader\"\r\n                        action=\"http://localhost:8081/file/upload\" \r\n                        :headers=\"{ token: user.token }\"\r\n                        :file-list=\"form.avatar ? [{ url: form.avatar }] : []\"  <!-- 注意此处改为数组对象 -->\r\n                        list-type=\"picture\"\r\n                        :on-success=\"handleAvatarSuccess\"\r\n                    >\r\n                        <el-button type=\"primary\">上传头像</el-button>\r\n                    </el-upload>\r\n                </el-form-item>\r\n            </el-form>\r\n    \r\n            <div slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"fromVisible = false\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n            </div>\r\n        </el-dialog>\r\n    </div>\r\n    </template>\r\n    \r\n    <script>\r\n    export default {\r\n        name: \"ShoppingView\",\r\n        data() {\r\n            return {\r\n                tableData: [],\r\n                pageNum: 1,   // 当前的页码\r\n                pageSize: 5,  // 每页显示的个数\r\n                goodsName: '',\r\n                total: 0,\r\n                fromVisible: false,\r\n                form: {\r\n                    goodsName: '',\r\n                    price: '',\r\n                    avatar: '' // 初始化 avatar\r\n                },\r\n                user: JSON.parse(localStorage.getItem('honey-user') || '{}'),\r\n                rules: {\r\n                    goodsName: [\r\n                        { required: true, message: '请输入商品名', trigger: 'blur' },\r\n                    ],\r\n                    price: [\r\n                        { required: true, message: '请输入价格', trigger: 'blur' },\r\n                    ]\r\n                },\r\n                ids: []\r\n            };\r\n        },\r\n        created() {\r\n            this.load();\r\n        },\r\n        \r\n        methods: {\r\n            delBatch() {\r\n                if (!this.ids.length) {\r\n                    this.$message.warning('请选择数据');\r\n                    return;\r\n                }\r\n                this.$confirm('您确认批量删除这些数据吗？', '确认删除', { type: \"warning\" })\r\n                    .then(() => {\r\n                        this.$request.delete('/goods/delete/batch', { data: this.ids }).then(res => {\r\n                            if (res.code === '200') {\r\n                                this.$message.success('操作成功');\r\n                                this.load(1);\r\n                            } else {\r\n                                this.$message.error(res.msg);\r\n                            }\r\n                        });\r\n                    }).catch(() => {});\r\n            },\r\n            handleSelectionChange(rows) {\r\n                this.ids = rows.map(v => v.id);\r\n            },\r\n            del(id) {\r\n                this.$confirm('您确认删除吗？', '确认删除', { type: \"warning\" })\r\n                    .then(() => {\r\n                        this.$request.delete('/goods/delete/' + id).then(res => {\r\n                            if (res.code === '200') {\r\n                                this.$message.success('操作成功');\r\n                                this.load(1);\r\n                            } else {\r\n                                this.$message.error(res.msg);\r\n                            }\r\n                        });\r\n                    }).catch(() => {});\r\n            },\r\n            handleEdit(row) {\r\n                this.form = JSON.parse(JSON.stringify(row));  // 深拷贝\r\n                this.fromVisible = true;   // 打开弹窗\r\n            },\r\n            handleAdd() {\r\n                this.form = { goodsName: '', price: '', avatar: '' }; // 新增时清空数据\r\n                this.fromVisible = true;   // 打开弹窗\r\n            },\r\n            save() {\r\n                this.$refs.formRef.validate((valid) => {\r\n                    if (valid) {\r\n                        this.$request({\r\n                            url: this.form.id ? '/file/update' : '/goods/add',\r\n                            method: this.form.id ? 'PUT' : 'POST',\r\n                            data: this.form\r\n                        }).then(res => {\r\n                            if (res.code === '200') {\r\n                                this.$message.success('保存成功');\r\n                                this.load(1);\r\n                                this.fromVisible = false;\r\n                            } else {\r\n                                this.$message.error(res.msg);\r\n                            }\r\n                        });\r\n                    }\r\n                });\r\n            },\r\n            reset() {\r\n                this.goodsName = '';\r\n                this.load();\r\n            },\r\n            load(pageNum) {\r\n                if (pageNum) this.pageNum = pageNum;\r\n                this.$request.get('/goods/selectByPage', {\r\n                    params: {\r\n                        pageNum: this.pageNum,\r\n                        pageSize: this.pageSize,\r\n                        goodsName: this.goodsName,\r\n                    }\r\n                }).then(res => {\r\n                    this.tableData = res.data.list;\r\n                    this.total = res.data.total; // 确保这里是 total\r\n                });\r\n            },\r\n            handleCurrentChange(pageNum) {\r\n                this.load(pageNum);\r\n            },\r\n            handleAvatarSuccess(response) {\r\n                // 将上传的图片链接赋给 form.avatar\r\n                this.form.avatar = response.data; // 确保 response.data 是图片的 URL\r\n            },\r\n        }\r\n    }\r\n    </script>\r\n    \r\n    <style scoped>\r\n    </style>\r\n    "],"mappings":"AA+DA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,SAAA;MACAC,KAAA;MACAC,WAAA;MACAC,IAAA;QACAH,SAAA;QACAI,KAAA;QACAC,MAAA;MACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,KAAA;QACAX,SAAA,GACA;UAAAY,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAV,KAAA,GACA;UAAAQ,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;MACAC,GAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EAEAC,OAAA;IACAC,SAAA;MACA,UAAAJ,GAAA,CAAAK,MAAA;QACA,KAAAC,QAAA,CAAAC,OAAA;QACA;MACA;MACA,KAAAC,QAAA;QAAAC,IAAA;MAAA,GACAC,IAAA;QACA,KAAAC,QAAA,CAAAC,MAAA;UAAA/B,IAAA,OAAAmB;QAAA,GAAAU,IAAA,CAAAG,GAAA;UACA,IAAAA,GAAA,CAAAC,IAAA;YACA,KAAAR,QAAA,CAAAS,OAAA;YACA,KAAAb,IAAA;UACA;YACA,KAAAI,QAAA,CAAAU,KAAA,CAAAH,GAAA,CAAAI,GAAA;UACA;QACA;MACA,GAAAC,KAAA;IACA;IACAC,sBAAAC,IAAA;MACA,KAAApB,GAAA,GAAAoB,IAAA,CAAAC,GAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAC,EAAA;IACA;IACAC,IAAAD,EAAA;MACA,KAAAf,QAAA;QAAAC,IAAA;MAAA,GACAC,IAAA;QACA,KAAAC,QAAA,CAAAC,MAAA,oBAAAW,EAAA,EAAAb,IAAA,CAAAG,GAAA;UACA,IAAAA,GAAA,CAAAC,IAAA;YACA,KAAAR,QAAA,CAAAS,OAAA;YACA,KAAAb,IAAA;UACA;YACA,KAAAI,QAAA,CAAAU,KAAA,CAAAH,GAAA,CAAAI,GAAA;UACA;QACA;MACA,GAAAC,KAAA;IACA;IACAO,WAAAC,GAAA;MACA,KAAAtC,IAAA,GAAAI,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAmC,SAAA,CAAAD,GAAA;MACA,KAAAvC,WAAA;IACA;IACAyC,UAAA;MACA,KAAAxC,IAAA;QAAAH,SAAA;QAAAI,KAAA;QAAAC,MAAA;MAAA;MACA,KAAAH,WAAA;IACA;IACA0C,KAAA;MACA,KAAAC,KAAA,CAAAC,OAAA,CAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAtB,QAAA;YACAuB,GAAA,OAAA9C,IA