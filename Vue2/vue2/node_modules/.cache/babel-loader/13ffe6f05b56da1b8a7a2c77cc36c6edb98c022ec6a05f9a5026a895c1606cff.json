{"ast":null,"code":"export default {\n  name: \"EnrollmentsView\",\n  data() {\n    return {\n      tableData: [],\n      pageNum: 1,\n      // 当前页码\n      pageSize: 5,\n      // 每页显示的条目数\n      course_code: '',\n      course_name: '',\n      total: 0,\n      ids: []\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    handleSelectionChange(rows) {\n      this.ids = rows.map(v => v.id);\n    },\n    toggleEnrollment(course) {\n      if (course.isEnrolled) {\n        this.cancelEnrollment(course);\n      } else {\n        this.enrollCourse(course);\n      }\n    },\n    enrollCourse(course) {\n      this.$request.post('/enrollments/add', {\n        student_id: this.user.id,\n        // 假设用户信息存储在 this.user 中\n        course_code: course.course_code\n      }).then(res => {\n        if (res.code === '200') {\n          this.$message.success('选课成功！');\n          course.isEnrolled = true; // 更新选课状态\n        } else {\n          this.$message.error(res.msg);\n        }\n      }).catch(err => {\n        this.$message.error('选课失败！');\n        console.error(err);\n      });\n    },\n    cancelEnrollment(course) {\n      this.$request.delete(`/enrollments/delete/${course.enrollment_id}`).then(res => {\n        if (res.code === '200') {\n          this.$message.success('取消选课成功！');\n          course.isEnrolled = false; // 更新选课状态\n        } else {\n          this.$message.error(res.msg);\n        }\n      }).catch(err => {\n        this.$message.error('取消选课失败！');\n        console.error(err);\n      });\n    },\n    load(pageNum) {\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/courses/selectByPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          course_code: this.course_code,\n          course_name: this.course_name\n        }\n      }).then(res => {\n        this.tableData = res.data.list.map(course => ({\n          ...course,\n          isEnrolled: Boolean(course.enrollment_id) // 根据 enrollment_id 判断是否已选课\n        }));\n        this.total = res.data.total;\n      }).catch(err => {\n        this.$message.error('加载课程失败！');\n        console.error(err);\n      });\n    },\n    reset() {\n      this.course_code = '';\n      this.course_name = '';\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","pageNum","pageSize","course_code","course_name","total","ids","created","load","methods","handleSelectionChange","rows","map","v","id","toggleEnrollment","course","isEnrolled","cancelEnrollment","enrollCourse","$request","post","student_id","user","then","res","code","$message","success","error","msg","catch","err","console","delete","enrollment_id","get","params","list","Boolean","reset","handleCurrentChange"],"sources":["src/views/manager/EnrollmentsView.vue"],"sourcesContent":["<template>\r\n    <div class=\"\">\r\n        <div>\r\n            <el-input style=\"width: 200px\" placeholder=\"查询课程代码\" v-model=\"course_code\"></el-input>\r\n            <el-input style=\"width: 200px; margin: 0 5px\" placeholder=\"查询课程名称\"  v-model=\"course_name\"></el-input>\r\n            <el-button type=\"primary\" @click=\"load(1)\">查询</el-button>\r\n            <el-button type=\"info\" @click=\"reset\">重置</el-button>\r\n        </div>\r\n\r\n        <el-table :data=\"tableData\" stripe :header-cell-style=\"{ backgroundColor: 'aliceblue', color: '#666' }\" @selection-change=\"handleSelectionChange\">\r\n            <el-table-column type=\"selection\" width=\"55\" align=\"center\"></el-table-column>\r\n            <el-table-column label=\"课程代码\" prop=\"course_code\" align=\"center\"></el-table-column>\r\n            <el-table-column label=\"课程名称\" prop=\"course_name\" align=\"center\"></el-table-column>\r\n            <el-table-column label=\"学分\" prop=\"credits\" align=\"center\"></el-table-column>\r\n            <el-table-column label=\"课程简介\" prop=\"description\" align=\"center\"></el-table-column>\r\n            <el-table-column label=\"学期\" prop=\"semester\" align=\"center\"></el-table-column>\r\n     \r\n            <el-table-column label=\"操作\" align=\"center\">\r\n                <template v-slot=\"scope\">\r\n                    <el-button \r\n                        size=\"mini\" \r\n                        :type=\"scope.row.isEnrolled ? 'danger' : 'primary'\" \r\n                        plain \r\n                        @click=\"toggleEnrollment(scope.row)\">\r\n                        {{ scope.row.isEnrolled ? '取消选课' : '选课' }}\r\n                    </el-button>\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n\r\n        <div style=\"margin: 10px 0\">\r\n            <el-pagination\r\n                @current-change=\"handleCurrentChange\"\r\n                :current-page=\"pageNum\"\r\n                :page-sizes=\"[5, 10, 20, 50]\"\r\n                :page-size=\"pageSize\"\r\n                layout=\"total, prev, pager, next\"\r\n                :total=\"total\">\r\n            </el-pagination>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: \"EnrollmentsView\",\r\n    data() {\r\n        return {\r\n            tableData: [],\r\n            pageNum: 1, // 当前页码\r\n            pageSize: 5, // 每页显示的条目数\r\n            course_code: '',\r\n            course_name: '',\r\n            total: 0,\r\n            ids: []\r\n        };\r\n    },\r\n    created() {\r\n        this.load();\r\n    },\r\n    methods: {\r\n        handleSelectionChange(rows) {\r\n            this.ids = rows.map(v => v.id);\r\n        },\r\n        toggleEnrollment(course) {\r\n            if (course.isEnrolled) {\r\n                this.cancelEnrollment(course);\r\n            } else {\r\n                this.enrollCourse(course);\r\n            }\r\n        },\r\n        enrollCourse(course) {\r\n            this.$request.post('/enrollments/add', {\r\n                student_id: this.user.id, // 假设用户信息存储在 this.user 中\r\n                course_code: course.course_code\r\n            }).then(res => {\r\n                if (res.code === '200') {\r\n                    this.$message.success('选课成功！');\r\n                    course.isEnrolled = true; // 更新选课状态\r\n                } else {\r\n                    this.$message.error(res.msg);\r\n                }\r\n            }).catch(err => {\r\n                this.$message.error('选课失败！');\r\n                console.error(err);\r\n            });\r\n        },\r\n        cancelEnrollment(course) {\r\n            this.$request.delete(`/enrollments/delete/${course.enrollment_id}`).then(res => {\r\n                if (res.code === '200') {\r\n                    this.$message.success('取消选课成功！');\r\n                    course.isEnrolled = false; // 更新选课状态\r\n                } else {\r\n                    this.$message.error(res.msg);\r\n                }\r\n            }).catch(err => {\r\n                this.$message.error('取消选课失败！');\r\n                console.error(err);\r\n            });\r\n        },\r\n        load(pageNum) {\r\n            if (pageNum) this.pageNum = pageNum;\r\n            this.$request.get('/courses/selectByPage', {\r\n                params: {\r\n                    pageNum: this.pageNum,\r\n                    pageSize: this.pageSize,\r\n                    course_code: this.course_code,\r\n                    course_name: this.course_name\r\n                }\r\n            }).then(res => {\r\n                this.tableData = res.data.list.map(course => ({\r\n                    ...course,\r\n                    isEnrolled: Boolean(course.enrollment_id) // 根据 enrollment_id 判断是否已选课\r\n                }));\r\n                this.total = res.data.total;\r\n            }).catch(err => {\r\n                this.$message.error('加载课程失败！');\r\n                console.error(err);\r\n            });\r\n        },\r\n        reset() {\r\n            this.course_code = '';\r\n            this.course_name = '';\r\n            this.load();\r\n        },\r\n        handleCurrentChange(pageNum) {\r\n            this.load(pageNum);\r\n        }\r\n    }\r\n};\r\n</script>\r\n\r\n\r\n<style scoped>\r\n</style>"],"mappings":"AA4CA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,WAAA;MACAC,WAAA;MACAC,KAAA;MACAC,GAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAC,sBAAAC,IAAA;MACA,KAAAL,GAAA,GAAAK,IAAA,CAAAC,GAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAC,EAAA;IACA;IACAC,iBAAAC,MAAA;MACA,IAAAA,MAAA,CAAAC,UAAA;QACA,KAAAC,gBAAA,CAAAF,MAAA;MACA;QACA,KAAAG,YAAA,CAAAH,MAAA;MACA;IACA;IACAG,aAAAH,MAAA;MACA,KAAAI,QAAA,CAAAC,IAAA;QACAC,UAAA,OAAAC,IAAA,CAAAT,EAAA;QAAA;QACAX,WAAA,EAAAa,MAAA,CAAAb;MACA,GAAAqB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAC,QAAA,CAAAC,OAAA;UACAZ,MAAA,CAAAC,UAAA;QACA;UACA,KAAAU,QAAA,CAAAE,KAAA,CAAAJ,GAAA,CAAAK,GAAA;QACA;MACA,GAAAC,KAAA,CAAAC,GAAA;QACA,KAAAL,QAAA,CAAAE,KAAA;QACAI,OAAA,CAAAJ,KAAA,CAAAG,GAAA;MACA;IACA;IACAd,iBAAAF,MAAA;MACA,KAAAI,QAAA,CAAAc,MAAA,wBAAAlB,MAAA,CAAAmB,aAAA,IAAAX,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAC,QAAA,CAAAC,OAAA;UACAZ,MAAA,CAAAC,UAAA;QACA;UACA,KAAAU,QAAA,CAAAE,KAAA,CAAAJ,GAAA,CAAAK,GAAA;QACA;MACA,GAAAC,KAAA,CAAAC,GAAA;QACA,KAAAL,QAAA,CAAAE,KAAA;QACAI,OAAA,CAAAJ,KAAA,CAAAG,GAAA;MACA;IACA;IACAxB,KAAAP,OAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAmB,QAAA,CAAAgB,GAAA;QACAC,MAAA;UACApC,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAC,WAAA,OAAAA,WAAA;UACAC,WAAA,OAAAA;QACA;MACA,GAAAoB,IAAA,CAAAC,GAAA;QACA,KAAAzB,SAAA,GAAAyB,GAAA,CAAA1B,IAAA,CAAAuC,IAAA,CAAA1B,GAAA,CAAAI,MAAA;UACA,GAAAA,MAAA;UACAC,UAAA,EAAAsB,OAAA,CAAAvB,MAAA,CAAAmB,aAAA;QACA;QACA,KAAA9B,KAAA,GAAAoB,GAAA,CAAA1B,IAAA,CAAAM,KAAA;MACA,GAAA0B,KAAA,CAAAC,GAAA;QACA,KAAAL,QAAA,CAAAE,KAAA;QACAI,OAAA,CAAAJ,KAAA,CAAAG,GAAA;MACA;IACA;IACAQ,MAAA;MACA,KAAArC,WAAA;MACA,KAAAC,WAAA;MACA,KAAAI,IAAA;IACA;IACAiC,oBAAAxC,OAAA;MACA,KAAAO,IAAA,CAAAP,OAAA;IACA;EACA;AACA","ignoreList":[]}