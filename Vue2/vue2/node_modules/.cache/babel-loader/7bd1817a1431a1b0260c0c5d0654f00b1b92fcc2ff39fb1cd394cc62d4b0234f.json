{"ast":null,"code":"export default {\n  name: \"TeachersView\",\n  data() {\n    return {\n      tableData: [],\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 5,\n      // 每页显示的个数\n      subject: \"\",\n      name: \"\",\n      total: 0,\n      fromVisible: false,\n      form: {},\n      user: JSON.parse(localStorage.getItem(\"honey-user\") || \"{}\"),\n      rules: {\n        teacher_id: [{\n          required: true,\n          message: \"请输入教师编号\",\n          trigger: \"blur\"\n        }],\n        name: [{\n          required: true,\n          message: \"请输入教师姓名\",\n          trigger: \"blur\"\n        }],\n        subject: [{\n          required: true,\n          message: \"请输入教授科目\",\n          trigger: \"blur\"\n        }]\n      },\n      ids: []\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    delBatch() {\n      if (!this.ids.length) {\n        this.$message.warning(\"请选择数据\");\n        return;\n      }\n      this.$confirm(\"您确认批量删除这些数据吗？\", \"确认删除\", {\n        type: \"warning\"\n      }).then(() => {\n        this.$request.delete(\"/teachers/delete/batch\", {\n          data: this.ids\n        }).then(res => {\n          if (res.code === \"200\") {\n            this.$message.success(\"操作成功\");\n            this.load();\n          } else {\n            this.$message.error(res.msg);\n          }\n        });\n      }).catch(() => {});\n    },\n    handleSelectionChange(rows) {\n      this.ids = rows.map(v => v.id);\n    },\n    del(id) {\n      this.$confirm(\"您确认删除吗？\", \"确认删除\", {\n        type: \"warning\"\n      }).then(() => {\n        this.$request.delete(`/teachers/delete/${id}`).then(res => {\n          if (res.code === \"200\") {\n            this.$message.success(\"操作成功\");\n            this.load(1);\n          } else {\n            this.$message.error(res.msg);\n          }\n        });\n      }).catch(() => {});\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.fromVisible = true;\n    },\n    handleAdd() {\n      this.form = {};\n      this.fromVisible = true;\n    },\n    save() {\n      this.$refs.formRef.validate(valid => {\n        if (valid) {\n          this.$request({\n            url: this.form.teacher_id ? \"/teachers/update\" : \"/teachers/add\",\n            method: this.form.teacher_id ? \"PUT\" : \"POST\",\n            data: this.form\n          }).then(res => {\n            if (res.code === \"200\") {\n              this.$message.success(\"保存成功\");\n              this.load(1);\n              this.fromVisible = false;\n            } else {\n              this.$message.error(res.msg);\n            }\n          });\n        }\n      });\n    },\n    reset() {\n      this.teacher_id = \"\";\n      this.name = \"\";\n      this.load();\n    },\n    load(pageNum) {\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get(\"/teachers/selectByPage\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name,\n          subject: this.subject\n        }\n      }).then(res => {\n        this.tableData = res.data.list;\n        this.total = res.data.total;\n      });\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","pageNum","pageSize","subject","total","fromVisible","form","user","JSON","parse","localStorage","getItem","rules","teacher_id","required","message","trigger","ids","created","load","methods","delBatch","length","$message","warning","$confirm","type","then","$request","delete","res","code","success","error","msg","catch","handleSelectionChange","rows","map","v","id","del","handleEdit","row","stringify","handleAdd","save","$refs","formRef","validate","valid","url","method","reset","get","params","list","handleCurrentChange"],"sources":["src/views/manager/TeachersView.vue"],"sourcesContent":["<template>\r\n    <div class=\"\">\r\n        <div>\r\n            <el-input style=\"width: 200px\" placeholder=\"查询教师姓名\" v-model=\"name\"></el-input>\r\n            <el-input style=\"width: 200px; margin: 0 5px\" placeholder=\"查询教师教学专业\" v-model=\"subject\"></el-input>\r\n            <el-button type=\"primary\" @click=\"load(1)\">查询</el-button>\r\n            <el-button type=\"info\" @click=\"reset\">重置</el-button>\r\n        </div>\r\n        <div style=\"margin: 10px 0\">\r\n            <el-button type=\"primary\" plain @click=\"handleAdd\">新增</el-button>\r\n            <el-button type=\"danger\" plain @click=\"delBatch\">批量删除</el-button>\r\n        </div>\r\n        <el-table\r\n            :data=\"tableData\"\r\n            stripe\r\n            :header-cell-style=\"{ backgroundColor: 'aliceblue', color: '#666' }\"\r\n            @selection-change=\"handleSelectionChange\"\r\n        >\r\n            <el-table-column type=\"selection\" width=\"55\" align=\"center\"></el-table-column>\r\n            <el-table-column label=\"教师编号\" prop=\"teacher_id\" align=\"center\"></el-table-column>\r\n            <el-table-column label=\"姓名\" prop=\"name\" align=\"center\"></el-table-column>\r\n            <el-table-column label=\"教授科目\" prop=\"subject\" align=\"center\"></el-table-column>\r\n            <el-table-column label=\"操作\" align=\"center\">\r\n                <template v-slot=\"scope\">\r\n                    <el-button size=\"mini\" type=\"primary\" plain @click=\"handleEdit(scope.row)\">编辑</el-button>\r\n                    <el-button size=\"mini\" type=\"danger\" plain @click=\"del(scope.row.teacher_id)\">删除</el-button>\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n\r\n        <div style=\"margin: 10px 0\">\r\n            <el-pagination\r\n                @current-change=\"handleCurrentChange\"\r\n                :current-page=\"pageNum\"\r\n                :page-sizes=\"[5, 10, 20, 50]\"\r\n                :page-size=\"pageSize\"\r\n                layout=\"total, prev, pager, next\"\r\n                :total=\"total\"\r\n            >\r\n            </el-pagination>\r\n        </div>\r\n\r\n        <el-dialog title=\"教师信息\" :visible.sync=\"fromVisible\" width=\"30%\">\r\n            <el-form :model=\"form\" label-width=\"80px\" style=\"padding-right: 20px\" :rules=\"rules\" ref=\"formRef\">\r\n                <el-form-item label=\"教师编号\" prop=\"teacher_id\">\r\n                    <el-input v-model=\"form.teacher_id\" placeholder=\"教师编号\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"姓名\" prop=\"name\">\r\n                    <el-input v-model=\"form.name\" placeholder=\"姓名\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"教授科目\" prop=\"subject\">\r\n                    <el-input v-model=\"form.subject\" placeholder=\"教授科目\"></el-input>\r\n                </el-form-item>\r\n            </el-form>\r\n\r\n            <div slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"fromVisible = false\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n            </div>\r\n        </el-dialog>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: \"TeachersView\",\r\n    data() {\r\n        return {\r\n            tableData: [],\r\n            pageNum: 1, // 当前的页码\r\n            pageSize: 5, // 每页显示的个数\r\n            subject: \"\",\r\n            name: \"\",\r\n            total: 0,\r\n            fromVisible: false,\r\n            form: {},\r\n            user: JSON.parse(localStorage.getItem(\"honey-user\") || \"{}\"),\r\n            rules: {\r\n                teacher_id: [{ required: true, message: \"请输入教师编号\", trigger: \"blur\" }],\r\n                name: [{ required: true, message: \"请输入教师姓名\", trigger: \"blur\" }],\r\n                subject: [{ required: true, message: \"请输入教授科目\", trigger: \"blur\" }],\r\n            },\r\n            ids: [],\r\n        };\r\n    },\r\n    created() {\r\n        this.load();\r\n    },\r\n    methods: {\r\n        delBatch() {\r\n            if (!this.ids.length) {\r\n                this.$message.warning(\"请选择数据\");\r\n                return;\r\n            }\r\n            this.$confirm(\"您确认批量删除这些数据吗？\", \"确认删除\", { type: \"warning\" })\r\n                .then(() => {\r\n                    this.$request.delete(\"/teachers/delete/batch\", { data: this.ids }).then((res) => {\r\n                        if (res.code === \"200\") {\r\n                            this.$message.success(\"操作成功\");\r\n                            this.load();\r\n                        } else {\r\n                            this.$message.error(res.msg);\r\n                        }\r\n                    });\r\n                })\r\n                .catch(() => {});\r\n        },\r\n        handleSelectionChange(rows) {\r\n            this.ids = rows.map((v) => v.id);\r\n        },\r\n        del(id) {\r\n            this.$confirm(\"您确认删除吗？\", \"确认删除\", { type: \"warning\" })\r\n                .then(() => {\r\n                    this.$request.delete(`/teachers/delete/${id}`).then((res) => {\r\n                        if (res.code === \"200\") {\r\n                            this.$message.success(\"操作成功\");\r\n                            this.load(1);\r\n                        } else {\r\n                            this.$message.error(res.msg);\r\n                        }\r\n                    });\r\n                })\r\n                .catch(() => {});\r\n        },\r\n        handleEdit(row) {\r\n            this.form = JSON.parse(JSON.stringify(row));\r\n            this.fromVisible = true;\r\n        },\r\n        handleAdd() {\r\n            this.form = {};\r\n            this.fromVisible = true;\r\n        },\r\n        save() {\r\n            this.$refs.formRef.validate((valid) => {\r\n                if (valid) {\r\n                    this.$request({\r\n                        url: this.form.teacher_id ? \"/teachers/update\" : \"/teachers/add\",\r\n                        method: this.form.teacher_id ? \"PUT\" : \"POST\",\r\n                        data: this.form,\r\n                    }).then((res) => {\r\n                        if (res.code === \"200\") {\r\n                            this.$message.success(\"保存成功\");\r\n                            this.load(1);\r\n                            this.fromVisible = false;\r\n                        } else {\r\n                            this.$message.error(res.msg);\r\n                        }\r\n                    });\r\n                }\r\n            });\r\n        },\r\n        reset() {\r\n            this.teacher_id = \"\";\r\n            this.name = \"\";\r\n            this.load();\r\n        },\r\n        load(pageNum) {\r\n            if (pageNum) this.pageNum = pageNum;\r\n            this.$request.get(\"/teachers/selectByPage\", {\r\n                params: {\r\n                    pageNum: this.pageNum,\r\n                    pageSize: this.pageSize,\r\n                    name: this.name,\r\n                    subject: this.subject,\r\n                },\r\n            }).then((res) => {\r\n                this.tableData = res.data.list;\r\n                this.total = res.data.total;\r\n            });\r\n        },\r\n        handleCurrentChange(pageNum) {\r\n            this.load(pageNum);\r\n        },\r\n    },\r\n};\r\n</script>\r\n\r\n<style scoped></style>\r\n"],"mappings":"AAgEA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,OAAA;MACAL,IAAA;MACAM,KAAA;MACAC,WAAA;MACAC,IAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,KAAA;QACAC,UAAA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAlB,IAAA;UAAAgB,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAb,OAAA;UAAAW,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MACA;MACAC,GAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAC,SAAA;MACA,UAAAJ,GAAA,CAAAK,MAAA;QACA,KAAAC,QAAA,CAAAC,OAAA;QACA;MACA;MACA,KAAAC,QAAA;QAAAC,I